        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCalculator"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Name="podaneRownanie" HorizontalAlignment="Left" Height="56" Margin="28,32,0,0" TextWrapping="Wrap" Text="21+3-3/3--3" VerticalAlignment="Top" Width="447" TextChanged="TextBox_TextChanged"/>
        <TextBlock Name="wynikPrzeliczenia" HorizontalAlignment="Left" Height="61" Margin="161,235,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="159"/>
        <Button Name="przyciskPrzeliczenia" Content="Przelicz" HorizontalAlignment="Left" Height="72" Margin="161,127,0,0" VerticalAlignment="Top" Width="159" Click="przyciskPrzeliczenia_Click"/>

    </Grid>
</Window>
